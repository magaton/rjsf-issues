{
    "downline": {
        "ui:order": [
            "type",
            "collapsingStrategy"
        ],
        "collapsingStrategy": {
            "clientTypes": {
                "ui:title": " ",
                "ui:widget": "CustomSelectComponent",
                "ui:options": {
                    "setAllOptionsByDefault": true,
                    "dependsOn": [
                        "client.types"
                    ],
                    "hideWidget": true
                }
            }
        },
        "type": {
            "ui:order": [
                "list",
                "default",
                "frontline",
                "group",
                "network"
            ],
            "default": {
                "ui:widget": "CustomSelectComponent",
                "ui:options": {
                    "dependsOn": "downline.type.list"
                }
            },
            "frontline": {
                "typeLevels": {
                    "ui:widget": "TypeLevelsCustomSelectComponent",
                    "ui:options": {
                        "setAllOptionsByDefault": true,
                        "pathToMinLevel": "downline.type.frontline.minLevel",
                        "pathToMaxLevel": "downline.type.frontline.maxLevel",
                        "pathToClientTypes": "client.types",
                        "pathToSponsorClientTypes": "client.sponsorTypes"
                    }
                }
            },
            "group": {
                "typeLevels": {
                    "ui:widget": "TypeLevelsCustomSelectComponent",
                    "ui:options": {
                        "setAllOptionsByDefault": true,
                        "pathToMinLevel": "downline.type.group.minLevel",
                        "pathToMaxLevel": "downline.type.group.maxLevel",
                        "pathToClientTypes": "client.types",
                        "pathToSponsorClientTypes": "client.sponsorTypes"
                    }
                }
            },
            "network": {
                "typeLevels": {
                    "ui:widget": "TypeLevelsCustomSelectComponent",
                    "ui:options": {
                        "setAllOptionsByDefault": true,
                        "pathToMinLevel": "downline.type.network.minLevel",
                        "pathToMaxLevel": "downline.type.network.maxLevel",
                        "pathToClientTypes": "client.types",
                        "pathToSponsorClientTypes": "client.sponsorTypes"
                    }
                }
            }
        }
    }
}